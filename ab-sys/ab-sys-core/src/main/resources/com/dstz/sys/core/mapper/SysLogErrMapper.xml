<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dstz.sys.core.mapper.SysLogErrMapper">

    <select id="query" parameterType="java.util.Map" resultType="com.dstz.sys.core.entity.SysLogErr">
        SELECT id_, account_, ip_, ip_address_, status_, url_, content_, create_time_ FROM sys_log_err
        <where>
            <if test="whereSql!=null">
                ${whereSql}
            </if>
        </where>
        <if test="orderBySql!=null">
            ORDER BY ${orderBySql}
        </if>
        <if test="orderBySql==null">
            ORDER BY id_ DESC
        </if>
    </select>
</mapper>
